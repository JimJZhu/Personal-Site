{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-param-reassign": "off",
    "no-mixed-operators": "off",
    "no-use-before-define": "off",
    "prefer-destructuring": "off",
    "func-names": "off",
    "no-loop-func": "off"
  }
}